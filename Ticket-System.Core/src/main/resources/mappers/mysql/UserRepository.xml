<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.apps.mybatis.mysql.UserInfoRepository">

    <select id="findByEmail" resultType="com.apps.domain.entity.UserInfo">
        select * from user_info where email = #{email}
    </select>




</mapper>